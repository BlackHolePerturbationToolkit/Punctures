\[Alpha]intmp2[p_Integer,l_Integer,m_Integer,\[Delta]_Symbol]/;EvenQ[m]:=\[Alpha]intmp2[p,l,m,\[Delta]]=Collect[modeintregmp2[p,l,m,norder,\[Delta]],Abs[\[Delta]],Simplify];


